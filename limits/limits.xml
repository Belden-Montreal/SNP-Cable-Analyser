<?xml version="1.0" encoding="utf-8"?>
<Root name="Standard">
	<Standard name="Tia">
		<Category name="CAT6A">
			<Hardware name="Connecting Hardware">
				<Limit param="RL">2</Limit>
				<Limit param="IL">3</Limit>
			</Hardware>
			<Hardware name="Cord Cable"></Hardware>
		</Category>
		<Category name="CAT6">
			<Hardware name="Connecting Hardware">
				<Limit param="RL">24-20*log(f/100)</Limit>
				<Limit param="IL">0.02*sqrt(f)</Limit>
				<Limit param="NEXT">54-20*log(f/100)</Limit>
				<Limit param="PSNEXT">50-20*log(f/100)</Limit>
				<Limit param="FEXT">43.1-20*log(f/100)</Limit>
				<Limit param="PSFEXT">40.1-20*log(f/100)</Limit>
				<Limit param="TCL">28-20*log(f/100)</Limit>
				<Limit param="TCTL">28-20*log(f/100)</Limit>
			</Hardware>
			<Hardware name="Horizontal cable">
				<Limit param="RL">20+5*log(f)</Limit>
				<Limit param="IL">1.8088*sqrt(f)+0.017*f+0.2/sqrt(f)</Limit>
				<Limit param="NEXT">44.3-15*log(f/100)</Limit>
				<Limit param="PSNEXT">42.3-15*log(f/100)</Limit>
				<Limit param="FEXT">43.1-20*log(f/100)</Limit>
				<Limit param="PSFEXT">24.8-20*log(f/100)</Limit>
				<Limit param="ACRF">27.8-20*log(f/100)</Limit>
				<Limit param="TCL">30-10*log(f/100)</Limit>
				<Limit param="ELTCTL">35-20*log(f)</Limit>
			</Hardware>
			<Hardware name="Cord Cable">
				<Limit param="RL">20+5*log(f)</Limit>
				<Limit param="IL">1.2*(1.808*sqrt(f)+0.017*f+0.2/sqrt(f))</Limit>
			</Hardware>
			<Hardware name="Backbone cable">
				<Limit param="RL">44.3-15*log(f/100)</Limit>
				<Limit param="PSNEXT">42.3-15*log(f/100)</Limit>
				<Limit param="ACRF">27.8-20*log(f/100)</Limit>
				<Limit param="PSACRF">24.8-20*log(f/100)</Limit>
			</Hardware>
			<Hardware name="Cords and jumpers">
				<Limit param="RL">24+3*log(f/25)</Limit>
			</Hardware>
		</Category>
		<Category name="CAT5e">
			<Hardware name="Connecting Hardware"></Hardware>
			<Hardware name="Cord Cable"></Hardware>
		</Category>
	</Standard>
